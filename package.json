{
  "name": "serialize-interceptor",
  "version": "1.1.5",
  "author": "nolleh <nolleh7707@gmail.com>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "pnpm run clean && pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "pnpm build",
    "start": "ts-node -r tsconfig-paths/register src/test/app.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "prepare": "husky install",
    "format": "prettier -w 'src/*.ts' '*.{json,md}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nolleh/serialize-interceptor"
  },
  "keywords": [
    "nestjs",
    "nest.js",
    "serialize",
    "deserialize",
    "camel",
    "snake",
    "json",
    "dto",
    "transform"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.0",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/core": "^8.4.0",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/swagger": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.0",
    "serialize-interceptor": "link:dist/cjs",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "src/.*\\.spec\\.(j|t)s$",
    "collectCoverageFrom": [
      "src/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,json,md}": "pnpm format"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  }
}
